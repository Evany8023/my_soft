create table SMZJ.BUSI_INVESTOR_FILE
(
  id          VARCHAR2(32) not null,
  investor_id VARCHAR2(32) not null,
  file_name   VARCHAR2(128 CHAR),
  file_type   VARCHAR2(32),
  file_url    VARCHAR2(128),
  create_by   VARCHAR2(128 CHAR),
  create_date DATE,
  update_by   VARCHAR2(128 CHAR),
  update_date DATE,
  is_delete   CHAR(1) default 0 not null
)
tablespace SMZJ
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
comment on column SMZJ.BUSI_INVESTOR_FILE.id
  is 'ID';
comment on column SMZJ.BUSI_INVESTOR_FILE.investor_id
  is '投资人ID';
comment on column SMZJ.BUSI_INVESTOR_FILE.file_name
  is '文件名';
comment on column SMZJ.BUSI_INVESTOR_FILE.file_type
  is '文件类型';
comment on column SMZJ.BUSI_INVESTOR_FILE.file_url
  is '图片地址';
comment on column SMZJ.BUSI_INVESTOR_FILE.create_by
  is '创建人';
comment on column SMZJ.BUSI_INVESTOR_FILE.create_date
  is '创建时间';
comment on column SMZJ.BUSI_INVESTOR_FILE.update_by
  is '更新人';
comment on column SMZJ.BUSI_INVESTOR_FILE.update_date
  is '更新时间';
comment on column SMZJ.BUSI_INVESTOR_FILE.is_delete
  is '是否删除（1：是，0：否）';
alter table SMZJ.BUSI_INVESTOR_FILE
  add constraint BUSI_INVESTOR_FILE_PK primary key (ID)
  using index 
  tablespace SMZJ
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );

